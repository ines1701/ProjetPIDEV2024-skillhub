{% extends 'baseback.html.twig' %}
{% block body %}
 <br/>
 <br/>
 <br/>
 <main id="main" class="main">
<section class="section">
<div class="row">
<div class="col-lg-12" >
<div class="card">
            <div class="card-body">
              <center><h5 class="card-title">Tous les  projets </h5></center>

              <!-- Table with hoverable rows -->
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Titre</th>
                    <th scope="col">Catégorie</th>
                    <th scope="col">Période</th>
                    <th scope="col">Budget</th>
                    <th scope="col">Portée</th>
                    <th scope="col">Déscription</th>
                    <th scope="col">Actions</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>  
                    {% for p in projet %} </tr>
                        <td>{{ p.id }}</td>
                        <td>{{ p.titre }}</td>
                        <td>{{ p.categorie }}</td>
                        <td> {{ p.periode }}</td>
                        <td> {{ p.budget }}</td>
                        <td> {{p.portee }}</td>
                        <td> {{p.description }}</td>
                        <td>
                            <a href="{{ path('app_deleteFA', {'id': p.id}) }}">Supprimer  </a>
                        </td>
                </tbody> {% endfor %}
              </table>
              <!-- End Table with hoverable rows -->
            </div>
          </div>
          </div>
          </div>

          
          </section>
          </main>
{% endblock %}